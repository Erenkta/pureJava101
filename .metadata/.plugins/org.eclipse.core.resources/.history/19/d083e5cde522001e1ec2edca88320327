
public class Variables {

	public static void main(String[] args) {
		//8 adet primitive type bulunmakta
		byte b;  //1 byte
		b = 10;
		System.out.println(b);
		
		short s = 20; //2 byte
		System.out.println(s);
		
		int i = 30; //4 byte
		System.out.println(i);
		
		long l = 40; //8 byte -> en büyüğüdür
		System.out.println(l);
	}

}
